@model List<SimpleEFApp.Models.Student>

<h2>Students</h2>
<a asp-action="Create">Add Student</a>

<table border="1">
    <tr>
        <th>Id</th>
        <th>Name</th>
        <th>Age</th>
    </tr>
@foreach (var s in Model)
{
    <tr>
        <td>@s.Id</td>
        <td>@s.Name</td>
        <td>@s.Age</td>
    </tr>
}
</table>
